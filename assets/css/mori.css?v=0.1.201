/*ImageBox*/
.image-box-plugin{position:fixed;top:0;left:0;text-align:center;width:100%;height:100%;background-color:rgba(253,246,243,0.9);z-index:999;display:none}
.image-box-plugin img{min-width:20%;max-width:100%;max-height:100%;cursor:-webkit-zoom-out;cursor:-moz-zoom-out;cursor: zoom-out;}
.image-box-plugin .image-box-container{position:absolute;top:50%;-webkit-transform:translate3D(0,-50%,0);transform:translate3D(0,-50%,0);width:100%;height:100%;text-align: center;display: -moz-box;-moz-box-align: center;-moz-box-pack: center;display: -webkit-box;-webkit-box-align: center;-webkit-box-pack: center;display: -o-box;-o-box-align: center;-o-box-pack: center;display: -ms-box;-ms-box-align: center;-ms-box-pack: center;display: box;box-align: center;box-pack: center;}
.ImageBox{max-width:100%;cursor:-webkit-zoom-in;cursor:-moz-zoom-in;cursor:zoom-in;transition: all 0.6s;}
.ImageBox:hover{
    transform: scale(1.03);
    -webkit-animation-timing-function:ease-in-out;
    -webkit-animation-name:breathe;
    -webkit-animation-duration:2700ms;
    -webkit-animation-iteration-count:infinite;
    -webkit-animation-direction:alternate;
}
@-webkit-keyframes breathe {
    0% {
        border:1px solid rgba(153, 196, 251, 0.8);
        box-shadow:0 1px 5px rgba(133, 213, 245, 0.8);
    }
    100% {
        border:1px solid rgba(153, 196, 251, 0.8);
        box-shadow:0 1px 25px rgba(133, 213, 245, 0.8);
    }
}

body.night .image-box-plugin{
    background-color: rgba(2,9,12,0.9);
}

/*friend-link*/
.friend-link-text {font-size:1.2em;text-align: center;}
#links{padding:5px 0;color:#cc0000}
#friend-link-main {list-style:none;overflow: hidden;padding-left: 15%;}
#friend-link-main li{float: left;width: 33.33%;margin-bottom: 20px;}
@media(max-width:800px){#friend-link-main li{width: 50%}}
#friend-link-main li:before {display: none !important;}
#friend-link-main li img{border-radius:100% !important;margin-bottom:5px;transition:0.5s;-webkit-transtion:0.5s}    
#friend-link-main li span{display:block !important}    
#friend-link-main li:hover img{transform:rotate(360deg);-webkit-transform:rotate(360deg);}

.comment_main img {
    max-height: 300px;
    max-width: 400px;
}

div#comments pre {
    max-height: 350px;
}
ol.page-navigator li {
    display: inline-block;
    float: none;
    margin: 0 .5em;
    background: rgba(248, 247, 241, 0.8);
    border: 2px solid #ddd;
    border-radius: 6px;
    width: 40px;
    height: 35px;
    text-align: center;
    transition: border .2s linear,color .25s linear,background-color .25s linear;
    cursor: pointer;
}
ol.page-navigator li:hover {
  border: 2px solid #bdc3c7;
}
ol.page-navigator li.current {
  background: rgba(225, 223, 212, 0.8);
}
ol.page-navigator li a {
    display: inline-block;
    width: 100%;
    padding: 0;
}
ol.page-navigator li.current a {
    color: #555;
}
body.night ol.page-navigator li {
  background: #333;
  border-color: #666;
}

/* PostTor CSS */
.post-tor-content {
  position: relative;
  z-index: 0;
  width: 100%;
  margin-right: auto!important;
  margin-left: auto!important;
  text-align: right;
}
.post-tor {
  margin-left: 20px;
  border-left: 1px solid rgba(221, 221, 221, 0.32);
  font-size: 12px;
  position: fixed;
  z-index: 0;
  top: 100px;
  display: inline-block;
  text-align: left;
}
