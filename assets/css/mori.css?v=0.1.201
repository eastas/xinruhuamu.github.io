    display: block;
    cursor: pointer;
    color: rgb(57, 57, 57);
    background-color: #fff;
    border: 2px solid rgb(57, 57, 57);
    padding: 0 4px;
    margin-bottom: 2px;
    line-height: 20px;
    transition: 0.2s;
    animation-duration: 30s;
    animation-iteration-count: infinite;
    animation-name: shake;
    animation-timing-function: ease-in-out;
}
